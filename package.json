{
  "type": "module",
  "name": "sveltekit-markdown-blog",
  "private": true,
  "scripts": {
    "dev": "bunx -bun vite dev",
    "build": "bun vite build && bun run scripts/fixroutes.ts",
    "preview": "bunx -bun vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "lint:fix": "prettier --plugin-search-dir . --write . && eslint . --fix",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.25.1",
    "@types/node": "^20.7.1",
    "@types/three": "^0.156.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.33.2",
    "mdsvex": "^0.11.0",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.2.1",
    "svelte-check": "^3.5.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  },
  "dependencies": {
    "@fontsource/jetbrains-mono": "^5.0.12",
    "@fontsource/manrope": "^5.0.12",
    "lucide-svelte": "^0.279.0",
    "open-props": "^1.5.16",
    "rehype-slug": "^6.0.0",
    "remark-toc": "^9.0.0",
    "remark-unwrap-images": "^4.0.0",
    "shiki": "^0.14.4",
    "three": "^0.156.1"
  }
}
